.top-menu-container
  margin 0
  padding 0
  list-style-type none
  text-align center

.top-menu-container ul li
  display inline

.top-menu-container ul li a
  text-decoration none
  padding .2em 1em
  color #fff
  background-color #036

.top-menu-container ul li a:hover
  color #fff
  background-color #369
